def factors(n):
    factor_list = []
    for i in range(1, int(n**0.5) + 1):
        if n % i == 0:
            factor_list.append(i)
            if n // i != i:
                factor_list.append(n // i)
    factor_list.sort()
    return factor_list
def find_factor(n, p):
    factor_list = factors(n)
    if p <= len(factor_list):
        return factor_list[p - 1]
    else:
        return 0
n = int(input())
p = int(input())
print(find_factor(n, p))
