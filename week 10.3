def two_sum_exists(nums, target):
    num_set = set()
    for num in nums:
        complement = target - num
        if complement in num_set:
            return True
        num_set.add(num)
    return False

# Accept input from the user
n = int(input())
nums = list(map(int, input().split()))
k = int(input())

# Check if two numbers sum up to k
if two_sum_exists(nums, k):
    print("Yes")
else:
    print("No")
