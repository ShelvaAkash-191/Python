def count_pairs(nums, k):
    count = 0
    n = len(nums)
    for i in range(n):
        for j in range(i+1, n):
            if abs(nums[i] - nums[j]) == k:
                count += 1
    return count

n =int(input())
li = input().split()
li = [int(x) for x in li]
k = int(input())
print(count_pairs(li,k))
